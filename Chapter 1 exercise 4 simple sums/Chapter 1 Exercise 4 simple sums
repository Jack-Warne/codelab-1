#include <iostream>
using namespace std;
int main() {
	// setting the variables 
	int no1 = 8; // whole number value so an integer
	int no2 = 10;  // whole number value so an integer
	int total;  // whole number value so an integer

	total = no1 + no2; // calculates the total

	// outputting the total
	cout << total << endl;

}
